{
    "name": "alt-three/twitter",
    "description": "A Twitter handle parser for Laravel 5",
    "keywords": ["twitter", "parser", "markdown", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Joseph Cohen",
            "email": "joe@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "league/commonmark": "^0.15"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^1.1",
        "graham-campbell/markdown": "^8.0",
        "graham-campbell/testbench": "^4.0",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Twitter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AltThree\\Tests\\Twitter\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "AltThree\\Twitter\\TwitterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
