{
    "name": "alt-three/twitter",
    "description": "A Twitter handle parser for Laravel 5",
    "keywords": ["twitter", "parser", "markdown", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alt Three",
            "email": "support@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "league/commonmark": "^0.17"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.0",
        "graham-campbell/markdown": "^9.0",
        "graham-campbell/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Twitter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AltThree\\Tests\\Twitter\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        },
        "laravel": {
            "providers": [
                "AltThree\\Twitter\\TwitterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
